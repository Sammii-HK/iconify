{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/web",
  "rewrites": [
    {
      "source": "/api/convert",
      "destination": "/api/convert"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
